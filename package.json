{
  "name": "progress_bar_for_react",
  "version": "1.0.9",
  "description": "This is a simple fully customizable react progress bar component",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --mode development --hot",
    "dist:demo": "PRODUCTION=true webpack --mode production --config webpack.prod.js ",
    "npm:publish:dist": "PRODUCTION=true webpack --mode production --config webpack.publish.js && npm publish",
    "test": "jest",
    "lint": "eslint",
    "preghdeploy": "npm run dist:demo",
    "ghdeploy": "gh-pages -d dist",
    "clean:install": "rm -rf node_modules && npm cache clean --force && npm install",
    "update:packages:ncu": "ncu -u && npm update && npm run clean:install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myapos/progress-bar-for-react.git"
  },
  "keywords": [
    "progress",
    "progressbar",
    "react",
    "javascript",
    "npm",
    "progress-bar-for-react",
    "progress-bar"
  ],
  "author": "Myron Apostolakis",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/myapos/progress-bar-for-react/issues"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "react-tooltip": "^4.2.6"
  },
  "peerDependencies": {},
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "homepage": "https://myapos.github.io/progress-bar-for-react",
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "npm-check-updates": "^6.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "webpack-merge": "^4.2.2",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "deploy": "^1.0.3",
