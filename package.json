{
  "name": "progress_bar_for_react",
  "version": "1.0.35",
  "description": "This is a simple fully customizable react progress bar component",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "FAST_REFRESH=false BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "scripts:build": "react-scripts build",
    "dist": "rollup -c",
    "publish": "npm publish",
    "clean:install": "rm -rf node_modules && npm cache clean --force && npm install",
    "update:packages:ncu": "ncu -u && npm update && npm run clean:install",
    "cypress:open:react:headless": "cypress run --config integrationFolder=./cypress/integration/react/ video=false",
    "cypress:open:react": "cypress open --config integrationFolder=./cypress/integration/react/ video=false",
    "cypress:headless": "cypress run",
    "test:react": "start-server-and-test start http://localhost:3000 cypress:open:react --browser chrome",
    "test:react:headless": "start-server-and-test start http://localhost:3000 cypress:open:react:headless",
    "report:summary": "nyc report --reporter=text-summary",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myapos/progress-bar-for-react.git"
  },
  "keywords": [
    "progress",
    "progressbar",
    "react",
    "javascript",
    "npm",
    "progress-bar-for-react",
    "progress-bar"
  ],
  "author": "Myron Apostolakis",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/myapos/progress-bar-for-react/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-react": "^7.12.10",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "0.0.9",
    "babel-loader": "^8.2.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-jss": "^10.9.0",
    "rollup": "^2.38.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "react-tooltip": "^4.2.21"
  }
}
