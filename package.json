{
  "name": "progress_bar_for_react",
  "version": "1.0.17",
  "description": "This is a simple fully customizable react progress bar component",
  "main": "dist/index.js",
  "scripts": {
    "start": "FAST_REFRESH=false BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "scripts:build": "react-scripts build",
    "dist": "NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src/lib --out-dir dist --copy-files",
    "publish": "npm run dist && npm publish",
    "preghdeploy": "npm run scripts:build",
    "ghdeploy": "gh-pages -d dist",
    "postghdeploy": "npm run npm:publish:dist",
    "clean:install": "rm -rf node_modules && npm cache clean --force && npm install",
    "update:packages:ncu": "ncu -u && npm update && npm run clean:install",
    "cypress:open:react:headless": "cypress run --config integrationFolder=./cypress/integration/react/ video=false",
    "cypress:headless": "cypress run",
    "test:react": "start-server-and-test start http://localhost:3000 cypress:open:react --browser chrome",
    "test:react:headless": "start-server-and-test start http://localhost:3000 cypress:open:react:headless",
    "report:summary": "nyc report --reporter=text-summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myapos/progress-bar-for-react.git"
  },
  "keywords": [
    "progress",
    "progressbar",
    "react",
    "javascript",
    "npm",
    "progress-bar-for-react",
    "progress-bar"
  ],
  "author": "Myron Apostolakis",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/myapos/progress-bar-for-react/issues"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.9.0",
    "react-scripts": "^5.0.0",
    "react-tooltip": "^4.2.21",
    "web-vitals": "^2.1.2"
  },
  "homepage": "https://myapos.github.io/progress-bar-for-react",
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.0",
    "deploy": "^1.0.3",
    "gh-pages": "^3.2.3",
    "istanbul-lib-coverage": "^3.2.0",
    "npm-check-updates": "^12.0.5",
    "nyc": "^15.1.0",
    "platform": "^1.3.6",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.0",
    "start-server-and-test": "^1.14.0",
    "versiony": "^2.0.1",
    "whatwg-fetch": "^3.6.2"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
