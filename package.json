    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^v4.0.0-alpha.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-css-modules": "^2.1.0",
    "jsdoc": "^3.6.4",
    "jsdom": "^16.2.2",
    "lint-staged": "^10.2.2",
