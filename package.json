{
  "name": "progress_bar_for_react",
  "version": "1.0.57",
  "description": "Made with create-react-library",
  "author": "Myron Apostolakis",
  "license": "MIT",
  "repository": "https://github.com/myapos/progress-bar-for-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build",
    "start": "vite",
    "vite:start:ts": "tsc && VITE_NODE_ENV=development vite",
    "ts": "tsc --noEmit",
    "prepare": "run-s build",
    "publish": "npm publish",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "clean:install": "rm -rf node_modules && npm cache clean --force && npm install",
    "cy": "cypress open",
    "cy:headless": "cypress run --config video=false",
    "test": "start-server-and-test storybook http://localhost:6006 cy --browser chrome",
    "test:headless": "start-server-and-test storybook http://localhost:6006 cy:headless",
    "storybook": "start-storybook -p 6006 @cypress/instrument-cra",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.1",
    "@cypress/code-coverage": "^3.12.30",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/webpack-dev-server": "^3.7.4",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@storybook/addon-actions": "^8.0.2",
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/addons": "^7.6.17",
    "@storybook/builder-webpack5": "^8.0.2",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.0.2",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/cypress": "^10.0.1",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.0",
    "cypress-real-events": "^1.12.0",
    "cypress-storybook": "^1.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "files": [
    "dist"
  ]
}
